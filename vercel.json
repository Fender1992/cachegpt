{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": ".next",
  "public": true,
  "crons": [
    {
      "path": "/api/cron/model-update",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/process-notifications",
      "schedule": "*/5 * * * *"
    }
  ],
  "functions": {
    "app/api/chat/route.ts": {
      "maxDuration": 30
    },
    "app/api/cron/model-update/route.ts": {
      "maxDuration": 10
    }
  }
}