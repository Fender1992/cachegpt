# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# LLM API Keys
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Hugging Face Configuration (for response adaptation)
HUGGINGFACE_API_KEY=hf_your_huggingface_api_key_here

# Security
JWT_SECRET=your_jwt_secret_here_use_a_secure_random_string

# Sentry Error Monitoring (optional)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
SENTRY_DSN=your_sentry_dsn_here
ENABLE_SENTRY_DEV=false

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_REQUESTS_PER_HOUR=1000

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Feature Flags
# Set to 'true' to enable enterprise mode where users can provide their own API keys
FEATURE_ENTERPRISE_USER_KEYS=false

# Additional LLM Provider Keys (server-side)
GOOGLE_AI_API_KEY=your_google_ai_key_here
PERPLEXITY_API_KEY=your_perplexity_key_here

# Free Provider API Keys (for server-side rotation)
GROQ_API_KEY=your_groq_api_key_here
TOGETHER_API_KEY=your_together_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Cron Secret (for scheduled jobs)
CRON_SECRET=your_cron_secret_here_use_a_secure_random_string

# Production URL
NEXT_PUBLIC_SITE_URL=https://cachegpt.app